{
  "name": "accounting-js",
  "version": "1.1.1",
  "description": "Number, money and currency formatting library.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "predoc": "rimraf docs",
    "doc": "esdoc -c esdoc.json",
    "build": "tsup",
    "lint": "eslint . --ext .js,.ts,.mjs",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "vitest run",
    "precoverage": "rimraf coverage",
    "coverage": "vitest run --coverage",
    "check-exports": "attw --pack ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nashdot/accounting-js"
  },
  "keywords": [
    "es6",
    "modules",
    "rollup",
    "bundle",
    "accounting",
    "number",
    "money",
    "currency",
    "format",
    "utilities",
    "finance",
    "exchange"
  ],
  "author": "Stanislav Lesnikov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nashdot/accounting-js/issues"
  },
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "homepage": "https://github.com/nashdot/accounting-js#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.1",
    "esdoc": "^0.5.2",
    "eslint": "^9.28.0",
    "rimraf": "^2.5.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.1"
  }
}
